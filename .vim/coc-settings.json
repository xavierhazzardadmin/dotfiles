{
    "eslint.enable": true,
    "eslint.autoFixOnSave": true,
    "suggest.echodocSupport": true,
    "suggest.maxCompleteItemCount": 20,
    "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "typescriptreact", "json", "javascriptreact", "typescript.tsx", "jst", "ejs", "yaml", "html", "css", "scss", "sass"],
    "eslint.run": "onSave",
    "eslint.probe": ["javascript", "typescript", "javascriptreact", "typescriptreact"],
    "eslint.trace.server": "verbose",
    "eslint.packageManager": "npm",
    "eslint.lintTask.options": [".", "--ext", ".ts"],
    "eslint.validate": ["javascript", "typescript", "javascriptreact", "typescriptreact", "typescript.tsx", "javascript.jsx"],
    "diagnostic.errorSign": "❌",
    "diagnostic.warningSign": "⚠",
    "diagnostic.infoSign": "‼",
    "tsserver.trace.server": "verbose",
    "vetur.format.enable": true,
    "vetur.format.defaultFormatter.html": "prettier",
    "vetur.format.defaultFormatter.css": "prettier",
    "tabnine.ignore_all_lsp": true,
    "javascript.suggest.autoImports": true,
    "typescript.suggest.autoImports": true
}
